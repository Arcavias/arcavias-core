#. Address plugin error: Payment address not available
msgid "address.payment.available.none"
msgstr "Не хватает адреса счета "

#. Address plugin error: Delivery address not available
msgid "address.delivery.available.none"
msgstr "Не хватает адреса доставки"

#. Address plugin error: Payment address not allowed
msgid "address.payment.available.notallowed"
msgstr "Адрес счета не разрешен"

#. Address plugin error: Delivery address not allowed
msgid "address.delivery.available.notallowed"
msgstr "Адрес доставки не разрешен"

#. Service plugin error: Payment service not available
msgid "service.payment.available.none"
msgstr "Не хватает опции для оплаты"

#. Service plugin error: Delivery service not available
msgid "service.delivery.available.none"
msgstr "Не хватает опции для пересылки"

#. Service plugin error: Payment service not allowed
msgid "service.payment.available.notallowed"
msgstr "Опция оплаты не разрешена"

#. Service plugin error: Delivery service not allowed
msgid "service.delivery.available.notallowed"
msgstr "Опция доставки не разрешена"

#. ProductGone plugin error: Product doesn't exists
msgid "product.gone.notexist"
msgstr "Извините, минимум один продукт больше не существует. Пожалуйста уберите его из вашей корзины, чтобы продолжить"

#. ProductGone plugin error: Product status disabled
msgid "product.gone.status"
msgstr "Извините, минимум один продукт был отключен. Пожалуйста уберите его из вашей корзины, чтобы продолжить"

#. ProductGone plugin error: Product out of timeframe
msgid "product.gone.timeframe"
msgstr "Извините, минимум один продукт больше не доступен. Пожалуйста уберите его из вашей корзины, чтобы продолжить"

#. ProductPrice plugin error: Product price changed
msgid "product.price.changed"
msgstr "Цена минимум одного продукта была изменина и обновлена автоматически"

#. ProductStock plugin error: Product out of stock
msgid "product.stock.notenough"
msgstr "Извините, минимум одного продукта больше нету в запасе. Пожалуйста уберите его из вашей корзины, чтобы продолжить"
